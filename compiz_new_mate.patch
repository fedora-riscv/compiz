-
-    themes = add_themes_from_dir (themes, METACITY_THEME_DIR);
-    themes = add_themes_from_dir (themes, "/usr/share/themes");
-    themes = add_themes_from_dir (themes, home_dir_themes);
-
-    g_free (home_dir_themes);
-
-    return themes;
-}
